<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.mywanandroid.ui.main.home.HomeFragment"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_tree"
        android:name="com.example.mywanandroid.ui.main.tree.TreeFragment"
        android:label="@string/title_tree"
        tools:layout="@layout/fragment_tree" />

    <fragment
        android:id="@+id/navigation_navi"
        android:name="com.example.mywanandroid.ui.main.navi.NaviFragment"
        android:label="@string/title_navi"
        tools:layout="@layout/fragment_navi" />

    <fragment
        android:id="@+id/navigation_mine"
        android:name="com.example.mywanandroid.ui.main.mine.MineFragment"
        android:label="@string/title_mine"
        tools:layout="@layout/fragment_mine" />
</navigation>